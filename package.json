{
  "name": "animated-menu-icon",
  "private": false,
  "version": "0.0.1",
  "type": "module",
  "description": "An animated menu icon that transforms to a close icon when menu is open",
  "main": "index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "author": "kgarrity22",
  "scripts": {
    "dev": "vite",
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "yarn  eslint",
      "yarn  format"
    ]
  },
  "dependencies": {
    "@commitlint/cli": "^19.3.0",
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@mui/material": "^5.16.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.0",
    "vite-plugin-react": "^4.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@types/node": "^22.7.4",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react": "^4.3.2",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^9.12.0",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^15.10.0",
    "husky": "^9.1.6",
    "prettier": "^3.3.3",
    "react-hooks": "^1.0.1",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.2.4",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.8.1",
    "vite": "^5.4.8",
    "vite-plugin-dts": "^4.2.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kgarrity22/mui-animated-close-menu-icon"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/kgarrity22/mui-animated-close-menu-icon/issues"
  },
  "homepage": "https://github.com/kgarrity22/mui-animated-close-menu-icon#readme",
  "license": "MIT"
}
